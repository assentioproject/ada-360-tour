	<style name="depthmap_button" type="text" css="text-align:center;" padding="4 8" mergedalpha="false" bgborder="0 0xFFFFFF 1" bgroundedge="1" bgshadow="0 0 4 0x000000 1.0" ondown="set(bgcolor, 0xDDDDDD);" onup="set(bgcolor, 0xFFFFFF);" />
	<style name="map-point" url="images/map-point.png" keep="true" align="lefttop" edge="center" scale="0.9" onover="set(scale,1.1);" onout="set(scale,0.9);"  />
	<style name="map-gps" url="images/HS_GPS.png" keep="true" align="lefttop" edge="bottom" scale="1" enabled="false"  />
	
	
	<layer name="kartecontainer"
       type="container"      
       keep="true"       
       visible="false"
       enabled="true"
       handcursor="false"     
       zorder=""
       align="left"
	   y="0"
       edge=""
	   children="true"
       maskchildren="true"
	   bgcolor="0xDDDDDD" 
	   bgborder="0 0xFFFFFF 1" bgroundedge="1" bgshadow="-1 -2 4 0x000000 1.0"
	   bgalpha="0.1" 
       width="200"
       height="100%"
       >
	   
	   <layer name="scrollarea"
       url.flash="plugins/scrollarea.swf"
       url.html5="plugins/scrollarea.js"
       align="center" width="200%" height="200%"
       direction="all"
       draggable="true"
	   handcursor="false"
	   parent="kartecontainer"
       capturechildren="true"
       parentmaskchildren="true"
       mwheel="false"
       onhover_autoscrolling="false"
       overscroll="1.0"
       friction="0.95"
       acceleration="0.08"
       returnacceleration="0.15"
       momentum="0.06"
       onscroll=""
       >
	   
	   
		<layer name="karte"
       type="image"
       url="images/map.JPG"
       keep="true"
       children="true" scalechildren="true" maskchildren="true"
       visible="true"
       enabled="true"
       handcursor="false"     
       zorder=""
       align="center"
       edge=""
       x="0"
       y="0"
       rotate="0.0"
       width="574"
       height="2339"
	   scale="0.7"
       >
	   
		
		<layer name="hs_gps" style="map-gps" parent="karte" zorder="5" x="59%" y="44%" />
	   
		
		<layer name="hs_Pano_04" style="map-point" parent="karte" zorder="4" x="47.7%" y="51.7%" 
			onclick="loadscene(scene_360_0004_Panorama, null, MERGE, BLEND(1));
			set(layer[hs_gps].x,47.7%); set(layer[hs_gps].y,51.7%);"
			/>
			
		<layer name="hs_Pano_05" style="map-point" parent="karte" zorder="4" x="52%" y="49%" 
			onclick="loadscene(scene_360_0005_Panorama, null, MERGE, BLEND(1));
			set(layer[hs_gps].x,52%); set(layer[hs_gps].y,49%);"
			/>
			
		<layer name="hs_Pano_06" style="map-point" parent="karte" zorder="4" x="27.6%" y="50.9%" 
			onclick="loadscene(scene_360_0006_Panorama, null, MERGE, BLEND(1));
			set(layer[hs_gps].x,27.6%); set(layer[hs_gps].y,50.9%);" 
			/>
			
		<layer name="hs_Pano_07" style="map-point" parent="karte" zorder="4" x="38%" y="55%" 
			onclick="loadscene(scene_360_0007_Panorama, null, MERGE, BLEND(1));
			set(layer[hs_gps].x,38%); set(layer[hs_gps].y,55%);" 
			/>
			

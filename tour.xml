	</layer>
	</layer> 
	</layer>
	
	<layer name="btncontainer"
       type="container"      
       keep="true"       
       visible="true"
       enabled="true"
       handcursor="false"     
       zorder="3"
       align="bottom"
       edge=""
	   children="true"
       maskchildren="true"
	   bgcolor="0xDDDDDD" 
	   bgborder="" bgroundedge="1" bgshadow=""
	   bgalpha="0.0"
       width="100%"
       height="40"
       >
			<layer name="BTN_Close_Map" keep="true" style="depthmap_button" html="Close Map" visible="false" parent="btncontainer" zorder="4" align="left" x="20" onclick="action(close_map);" />	
			<layer name="BTN_Map" keep="true" style="depthmap_button" html="Map" visible="true" parent="btncontainer" zorder="4" align="left" x="20" onclick="action(open_map);" />
			<layer name="BTN_FS" keep="true" devices="!ios" style="depthmap_button" html="Fullscreen" visible="true" parent="btncontainer" zorder="4" align="right" x="20" onclick="toggle(fullscreen);" />
	 
			
	   </layer>
	  
		
		
	
	 <action name="close_map">
		set(layer[kartecontainer].visible,false);
		set(layer[BTN_Map].visible,true);
		set(layer[BTN_Close_Map].visible,false);
	</action>
	
	<action name="open_map">
		set(layer[kartecontainer].visible,true);
		set(layer[BTN_Map].visible,false);
		set(layer[BTN_Close_Map].visible,true);
	</action>


</krpano>
